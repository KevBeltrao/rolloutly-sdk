{
  "name": "@rolloutly/core",
  "version": "0.1.4",
  "description": "Rolloutly feature flags SDK - Core JavaScript client",
  "author": "Kevin Beltr√£o",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KevBeltrao/rolloutly-sdk.git",
    "directory": "packages/core"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "rolloutly",
    "sdk"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "firebase": "^10.7.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
